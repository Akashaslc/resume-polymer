{
  "hosting": {
    "public": ".",
    "rewrites": [{
        "source": "!/__/**",
        "destination": "/index.html"
      },
      {
        "source": "**/!(*.js|*.html|*.css|*.json|*.svg|*.png|*.jpg|*.jpeg)",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source" : "**/*.@(jpg|jpeg|gif|png|js|css)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=3600"
      }]
    }]
  }
}
